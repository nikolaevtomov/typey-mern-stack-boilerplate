{
  "name": "client",
  "main": "src/index.tsx",
  "scripts": {
    "build": "npm run build:all",
    "build:all": "npm run build:clean && npm run build:js",
    "build:clean": "rm -rf dist && mkdir dist",
    "build:js": "cross-env NODE_ENV=production DEBUG=false webpack --mode production --progress --colors",
    "lint": "tsc --noEmit && eslint 'src/*.{ts,tsx,js,jsx,css,less,json}' --quiet --fix",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "start:dev": "cross-env NODE_ENV=development DEBUG=true webpack --watch --mode development"
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/history": "^4.7.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.7",
    "babel-loader": "^8.1.0",
    "babel-plugin-const-enum": "^1.0.1",
    "connected-react-router": "^6.8.0",
    "core-js": "^3.6.5",
    "history": "^4.10.1",
    "immutable": "^4.0.0-rc.12",
    "normalize-scss": "^7.0.1",
    "raf": "^3.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {}
}
